<div class="row">
 <h3> Learn Online: Pick the Set </h3>
</div>
<div class="row">
<%@question_sets.each do |set| %>
  <div class="col-md-3">
    <div class="qsetcards">
      <div class="title">
        <b><%=set.name%></b>
      </div>
      <div class="count">
        <%=@all_questions[set.id].count %>
        <% if @all_questions[set.id].count == 1 %>
          <%="Question"%>
        <% else %>
          <%= "Questions" %>
        <% end %>
        <div class="correct">
          <%=@correct[set.id]%><%='%' if @correct[set.id] != 'Not Started' %>
        </div>
      </div>
    </div>
  </div>
  <%end%>
</div>

